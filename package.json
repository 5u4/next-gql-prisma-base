{
  "name": "next-gql-prisma-base",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "postinstall": "yarn generate",
    "check": "tsc --noEmit",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "generate": "run-s generate:*",
    "generate:prisma": "prisma generate",
    "generate:codegen": "graphql-codegen --config codegen.yml"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "yarn ts-node src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@urql/core": "^2.4.0",
    "@urql/exchange-graphcache": "^4.3.6",
    "@urql/exchange-persisted-fetch": "^1.3.4",
    "ajv": "^8.9.0",
    "apollo-server-micro": "^3.6.2",
    "apollo-server-plugin-response-cache": "^3.5.1",
    "graphql": "^15.8.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.14.1",
    "graphql-tag": "^2.12.6",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.0.9",
    "next-i18next": "^10.2.0",
    "nexus": "^1.2.0",
    "nexus-prisma": "^0.35.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "urql": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-urql": "^3.4.2",
    "@graphql-codegen/urql-introspection": "^2.1.1",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "17.0.13",
    "@types/react": "17.0.38",
    "@urql/devtools": "^2.0.3",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.9",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.8.1",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
  }
}
